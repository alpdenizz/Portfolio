+        }
+
+        // pad the PIN and use the chip for verification
+        byte[] paddedPIN = Hex.decode("ffffffffffffffffffffffff");
+        System.arraycopy(PIN, 0, paddedPIN, 0, PIN.length);
+        switch(codeType) {
+            case PIN1 -> {
+                byte[] response = getResponse(paddedPIN, verifyPIN1, "verify PIN1");
+                byte sw1 = response[response.length - 2];
+                byte sw2 = response[response.length - 1];
+                if (sw1 != (byte) 0x90 || sw2 != 0x00) {
+                    if(sw1 == (byte) 0x63 || ((sw1 == 0x69) && sw2 == (byte) 0x83)) {
