+            case PIN2 -> {
+                byte[] response = getResponse(paddedPIN, verifyPIN2, "verify PIN2");
+                byte sw1 = response[response.length - 2];
+                byte sw2 = response[response.length - 1];
+                if (sw1 != (byte) 0x90 || sw2 != 0x00) {
+                    if(sw1 == (byte) 0x63 || ((sw1 == 0x69) && sw2 == (byte) 0x83)) {
+                        throw new CodeVerificationException(CodeType.PIN2);
+                    }
+                    throw new RuntimeException("Verify PIN2 unexpected status word: "+Hex.toHexString(new byte[]{sw1, sw2}));
+                }
+            }
+            case PUK -> {
+                byte[] response = getResponse(paddedPIN, verifyPUK, "verify PUK");
+                byte sw1 = response[response.length - 2];
+                byte sw2 = response[response.length - 1];
+                if (sw1 != (byte) 0x90 || sw2 != 0x00) {
+                    if(sw1 == (byte) 0x63 || ((sw1 == 0x69) && sw2 == (byte) 0x83)) {
+                        throw new CodeVerificationException(CodeType.PUK);
+                    }
+                    throw new RuntimeException("Verify PUK unexpected status word: "+Hex.toHexString(new byte[]{sw1, sw2}));
