+
+    public final Observable<IdCardDataResponse> dataUSB() {
+        return smartCardReaderManager.status()
+                .filter(duplicates())
+                .switchMap(status -> {
+                    if (status.equals(SmartCardReaderStatus.IDLE)) {
+                        return Observable.just(IdCardDataResponse.initial(false));
+                    } else if (status.equals(SmartCardReaderStatus.READER_DETECTED)) {
+                        return Observable.just(IdCardDataResponse.readerDetected(false));
+                    }
+                    return Observable
+                            .fromCallable(() -> {
+                                Token token = Token.create(smartCardReaderManager.connectedReader());
+                                Timber.log(Log.DEBUG, "[+] [dataUSB()] current status: %s", status);
+                                Timber.log(Log.DEBUG, "[+] [dataUSB()] reads ID card information");
+                                return IdCardDataResponse.success(data(token), token, false);
+                            })
+                            .subscribeOn(Schedulers.io())
+                            .startWithItem(IdCardDataResponse.cardDetected(false));
+                })
+                .onErrorReturn(error -> IdCardDataResponse.failure(error,false))
+                .observeOn(AndroidSchedulers.mainThread());
+    }
+
+    public void disableNFCReader() {
+        Timber.log(Log.DEBUG, "[+] disable NFC reader");
+        nfcManager.disable(navigator.activity());
+    }
+
+    public final Observable<IdCardDataResponse> dataNFC(String CAN, boolean doOperation, boolean forMyeID) {
+        settingsDataStore.setCAN(CAN);
+        return nfcManager.status()
+                .filter(duplicates())
+                .switchMap(status -> {
+                    if(status.equals(SmartCardReaderStatus.IDLE)){
+                        Timber.log(Log.DEBUG, "[+] NFC reader becomes IDLE (not expected unless NFC is not enabled)");
+                        return Observable.just(IdCardDataResponse.initial(true));
+                    }
+                    NFCToken token = new NFCToken(nfcManager.currentAdapter(), navigator.activity());
+                    if(settingsDataStore.getCAN().length() != 6) {
+                        Timber.log(Log.DEBUG, "[+] Personal data not cached, request CAN...");
+                        return Observable.just(IdCardDataResponse.cardWaitCAN(token,null));
+                    }
+                    Optional<IdCardData> cached = settingsDataStore.getCachedIdCardData();
+                    Timber.log(Log.DEBUG,"[+] cached data exists: %s",cached.isPresent());
+                    if(!doOperation && cached.isPresent()) {
+                        Timber.log(Log.DEBUG,"[+] Return cached data except for my eid");
+                        return Observable.just(IdCardDataResponse.success(cached.get(), token, true));
+                    }
+                    Timber.log(Log.DEBUG,"[+] Enable NFC reader and start observing IdCardDataResponse for NFC");
+                    return token.observeIdCardDataResponse(settingsDataStore.getCAN(),cached,forMyeID)
+                            .filter(duplicates())
+                            .map(res -> {
+                                if(res.status().equals(SmartCardReaderStatus.CARD_WAIT_CAN)) {
+                                    Timber.log(Log.DEBUG,"[+] IdCardService card wait can disable nfc");
+                                    disableNFCReader();
+                                    if(res.error() != null) {
+                                        Timber.log(Log.DEBUG,"[+] Error during CAN verification: %s",res.error());
+                                        settingsDataStore.clearCachedIdCardData();
+                                    }
+                                }
+                                if(res.data() != null) {
+                                    Timber.log(Log.DEBUG,"[+] response with data, saving data to preferences...");
+                                    IdCardData idCardData = res.data();
+                                    settingsDataStore.setAuthCertificate(idCardData.authCertificate().data().toByteArray());
+                                    //Timber.log(Log.DEBUG,"[+] stored auth cert length: %d",settingsDataStore.getAuthCertificate().length);
+                                    settingsDataStore.setSignCertificate(idCardData.signCertificate().data().toByteArray());
+                                    //Timber.log(Log.DEBUG,"[+] stored sign cert length: %d",settingsDataStore.getSignCertificate().length);
+                                    settingsDataStore.setPinCounts(idCardData.pin1RetryCount(), idCardData.pin2RetryCount(), idCardData.pukRetryCount());
+                                    int[] pinCounts = settingsDataStore.getPinCounts();
+                                    Timber.log(Log.DEBUG,"[+] stored pin counts; pin1: %d, pin2: %d, puk %d", pinCounts[0], pinCounts[1], pinCounts[2]);
+                                    PersonalData pd = idCardData.personalData();
+                                    settingsDataStore.setPersonalData(pd.surname(), pd.givenNames(), pd.personalCode());
+                                }
+                                return res;
+                            })
+                            .subscribeOn(Schedulers.io());
+                })
