+    public Observable<IdCardDataResponse> certRequestResponse() {
+        return Observable.fromCallable(() -> {
+            IdCardData cached = settingsDataStore.getCachedIdCardData().get();
+            NFCToken token = new NFCToken(nfcManager.currentAdapter(), navigator.activity());
+            return IdCardDataResponse.success(cached, token, true);
