+    private void handleDeepLink(Intent intent) {
+        if(intent.getData() == null || intent.getData().getScheme() == null || intent.getData().getHost() == null) return;
+        Uri link;
+        if (intent.getData().getScheme().equals("webeid")) {
+            link = intent.getData();
+        } else if (intent.getData().getHost().equals("acs.cs.ut.ee")) {
+            String requestFragment = intent.getData().getFragment();
+            Timber.log(Log.INFO,"[incoming webeid request fragment] %s",requestFragment);
+            //authenticate?query
+            int firstQ = requestFragment.indexOf("?");
+            String host = requestFragment.substring(0,firstQ);
+            Timber.log(Log.INFO,"[incoming webeid request host] %s",host);
+            String query = requestFragment.substring(firstQ+1);
+            Timber.log(Log.INFO,"[incoming webeid request query] %s",query);
+            link = new Uri.Builder()
+                    .scheme("webeid")
+                    .authority(host)
+                    .encodedQuery(query).build();
+        } else {
+            return;
+        }
+        Timber.log(Log.INFO,"[incoming webeid request] %s",link.toString());
+        String callerHost = link.getHost();
+        String lang = link.getQueryParameter("lang");
+        String userInteractionTimeout = link.getQueryParameter("userInteractionTimeout");
+        if(callerHost.equals("authenticate")) {
+            String challenge = link.getQueryParameter("challengeNonce");
+            String callbackUrl = link.getQueryParameter("callbackUrl");
+            String getSigningCertificate = link.getQueryParameter("getSigningCertificate");
+            Uri uri = Uri.parse(callbackUrl);
+            intent.setData(null);
+            intent.putExtra("caller",uri.getScheme()+"://"+uri.getHost());
+            intent.putExtra("callbackUrl",uri.toString());
+            intent.putExtra("auth_challenge",challenge);
+            intent.putExtra("userInteractionTimeout", userInteractionTimeout);
+            intent.putExtra("getSigningCertificate", getSigningCertificate);
+            intent.setIdentifier("webeidauth");
+            intent.setType("application/auth");
+        } else if(callerHost.equals("getSigningCertificate")) {
+            String callbackUrl = link.getQueryParameter("callbackUrl");
+            Uri uri = Uri.parse(callbackUrl);
+            intent.setData(null);
+            intent.putExtra("lang", lang);
+            intent.putExtra("userInteractionTimeout", userInteractionTimeout);
+            intent.putExtra("caller",uri.getScheme()+"://"+uri.getHost());
+            intent.putExtra("callbackUrl",uri.toString());
+            intent.setIdentifier("getsigningcertificate");
+            intent.setType("application/getsigningcertificate");
+        } else if(callerHost.equals("sign")) {
+            String callbackUrl = link.getQueryParameter("callbackUrl");
+            String hash = link.getQueryParameter("hash");
+            String hashFunction = link.getQueryParameter("hashFunction");
+            String certificate = link.getQueryParameter("certificate");
+            Uri uri = Uri.parse(callbackUrl);
+            intent.setData(null);
+            intent.putExtra("lang", lang);
+            intent.putExtra("userInteractionTimeout", userInteractionTimeout);
+            intent.putExtra("caller",uri.getScheme()+"://"+uri.getHost());
+            intent.putExtra("callbackUrl",uri.toString());
+            intent.putExtra("hash", hash);
+            intent.putExtra("hashFunction", hashFunction);
+            intent.putExtra("certificate", certificate);
+            intent.setIdentifier("webeidsign");
+            intent.setType("application/sign");
+        }
+    }
