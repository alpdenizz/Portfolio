+                                    return Observable.just(Result.ChooseFilesResult.create());
+                                }
+                                String hashFunction = activityResult.data().getStringExtra("hashFunction");
+                                if(hashFunction == null) {
+                                    String callback = activityResult.data().getStringExtra("callbackUrl");
+                                    Intent intent = errorCallbackWebEID(callback, "WEBEID_INVALID_PARAM", "hashFunction must be set");
+                                    Activity.stopTimeoutTimer();
+                                    navigator.activity().startActivity(intent.setFlags(Intent.FLAG_ACTIVITY_NEW_TASK|Intent.FLAG_ACTIVITY_CLEAR_TOP));
+                                    navigator.activity().finishAndRemoveTask();
+                                    return Observable.just(Result.ChooseFilesResult.create());
+                                }
+                                String certificate = activityResult.data().getStringExtra("certificate");
+                                if(certificate == null) {
+                                    String callback = activityResult.data().getStringExtra("callbackUrl");
+                                    Intent intent = errorCallbackWebEID(callback, "WEBEID_INVALID_PARAM", "certificate must be set");
+                                    Activity.stopTimeoutTimer();
+                                    navigator.activity().startActivity(intent.setFlags(Intent.FLAG_ACTIVITY_NEW_TASK|Intent.FLAG_ACTIVITY_CLEAR_TOP));
+                                    navigator.activity().finishAndRemoveTask();
+                                    return Observable.just(Result.ChooseFilesResult.create());
+                                }
+                                try {
+                                    Base64.getUrlDecoder().decode(hash);
+                                    Base64.getUrlDecoder().decode(certificate);
+                                }catch(Exception e) {
+                                    String callback = activityResult.data().getStringExtra("callbackUrl");
+                                    Intent intent = errorCallbackWebEID(callback, "WEBEID_INVALID_PARAM", "hash and certificate must be Base64URL encoded");
+                                    Activity.stopTimeoutTimer();
+                                    navigator.activity().startActivity(intent.setFlags(Intent.FLAG_ACTIVITY_NEW_TASK|Intent.FLAG_ACTIVITY_CLEAR_TOP));
+                                    navigator.activity().finishAndRemoveTask();
+                                    return Observable.just(Result.ChooseFilesResult.create());
+                                }
+                                Map<String,String> parameters = new HashMap<>();
+                                parameters.put("hash",hash);
+                                parameters.put("hashFunction",hashFunction);
+                                parameters.put("certificate",certificate);
+                                parameters.put("origin",activityResult.data().getStringExtra("caller"));
+                                parameters.put("callbackUrl", callbackUrl);
+                                parameters.put("purpose","sign");
+                                navigator.execute(Transaction.replace(SignatureUpdateScreen.create(true, true, parameters)));
+                                return Observable.just(Result.ChooseFilesResult.create());
+                            }
+                        }
+                        //end
+                        if (activityResult.data() != null) {
+                            ImmutableList<FileStream> validFiles = FileSystem.getFilesWithValidSize(
+                                    parseGetContentIntent(navigator.activity(), application.getContentResolver(), activityResult.data(), fileSystem.getExternallyOpenedFilesDir()));
+                            ToastUtil.handleEmptyFileError(validFiles, navigator.activity());
+
+                            return handleFiles(navigator, signatureContainerDataSource, validFiles)
+                                    .observeOn(AndroidSchedulers.mainThread())
+                                    .subscribeOn(AndroidSchedulers.mainThread())
+                                    .doOnError(throwable1 -> {
+                                        Timber.log(Log.ERROR, throwable1,
+                                                String.format("Unable to add file to container. Error: %s",
+                                                        throwable1.getLocalizedMessage()));
+                                        ToastUtil.showError(navigator.activity(), R.string.signature_create_error);
+
+                                        navigator.execute(Transaction.pop());
+                                    });
+                        } else {
+                            Timber.log(Log.ERROR, "Data from file chooser is empty");
+                            ToastUtil.showError(navigator.activity(), R.string.signature_create_error);
+
+                            navigator.execute(Transaction.pop());
+                        }
+                    } else {
+                        if (ActivityUtil.isExternalFileOpened(navigator.activity())) {
+                            ActivityUtil.restartActivity(application.getApplicationContext(), navigator.activity());
+                        } else {
+                            navigator.execute(Transaction.pop());
+                        }
+                        return Observable.just(Result.ChooseFilesResult.create());
+                    }
+
+                    return Observable.empty();
+                })
+                .onErrorReturn(throwable -> {
+                    List<Throwable> exceptions = ((CompositeException) throwable).getExceptions();
+                    if (!exceptions.isEmpty()) {
+                        boolean isEmptyFileException = exceptions.stream().anyMatch(exception ->
+                                (exception instanceof EmptyFileException));
+                        if (isEmptyFileException) {
+                            ToastUtil.showEmptyFileError(navigator.activity());
+                        } else {
+                            ToastUtil.showError(navigator.activity(), R.string.signature_create_error);
