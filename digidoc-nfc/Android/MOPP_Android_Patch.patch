+    private void verifyPIN(byte[] PIN, CodeType codeType) throws NoSuchPaddingException, InvalidAlgorithmParameterException, NoSuchAlgorithmException, IllegalBlockSizeException, BadPaddingException, InvalidKeyException, IOException, CodeVerificationException {
