+    //maybe move this to add source as single callable like in signature
+    private byte[] tobesignedData(Map<String,String> authRequest) throws Exception {
+        //calculate hash(origin)+hash(challenge) and sign it
+        Timber.log(Log.DEBUG, "[+] auth challenge: %s",authRequest.get("challenge"));
+        byte[] challenge = Base64.getUrlDecoder().decode(authRequest.get("challenge"));
+        Timber.log(Log.DEBUG, "[+] auth origin: %s",authRequest.get("origin"));
+        byte[] origin = authRequest.get("origin").getBytes(StandardCharsets.UTF_8);
+        MessageDigest md = MessageDigest.getInstance("SHA-384");
+        byte[] hashedChallenge = md.digest(challenge);
+        byte[] hashedOrigin = md.digest(origin);
+        md.update(hashedOrigin);
+        md.update(hashedChallenge);
+        return md.digest();
+    }
