+    //removed dataUSB() to support only nfc in webeid
+    public final Observable<IdCardDataResponse> data(boolean doOperation,boolean forMyeID) {
+        Timber.log(Log.DEBUG, "[+] START observing both NFC and USB readers");
+        return dataNFC(settingsDataStore.getCAN(),doOperation,forMyeID)
+                .doOnNext(r -> Timber.log(Log.DEBUG, "[+] [data()] current response: %s", r))
+                .map(response -> {
+                    if(forMyeID) return response;
+                    if(!response.status().equals(SmartCardReaderStatus.IDLE)) return response;
+                    Optional<IdCardData> cached = settingsDataStore.getCachedIdCardData();
+                    Timber.log(Log.DEBUG,"[+] cached data exists: %s",cached.isPresent());
+                    if(cached.isPresent()) {
+                        Timber.log(Log.DEBUG,"[+] Return cached data except for my eid");
+                        NFCToken token = new NFCToken(nfcManager.currentAdapter(), navigator.activity());
+                        return IdCardDataResponse.success(cached.get(), token, true);
+                    }
+                    return response;
+                });
+    }
