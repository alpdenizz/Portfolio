+package ee.beta.DigiDoc.android.nfc;
+
+import android.app.Activity;
+import android.content.Context;
+import android.nfc.NfcAdapter;
+import android.util.Log;
+
+import java.util.concurrent.TimeUnit;
+
+import ee.beta.DigiDoc.smartcardreader.SmartCardReaderStatus;
+import io.reactivex.rxjava3.core.Observable;
+import timber.log.Timber;
+
+public final class NFCManager {
+
+    private static final int CONNECT_RETRY = 3000;
+    private final Observable<SmartCardReaderStatus> statusObservable;
+    private NfcAdapter adapter;
+
+    public NFCManager(Context context) {
+        statusObservable = Observable.fromCallable(() ->
+                        NfcAdapter.getDefaultAdapter(context.getApplicationContext())
+                ).repeatWhen(completed -> completed.delay(CONNECT_RETRY, TimeUnit.MILLISECONDS))
+                .map(res -> {
+                    adapter = res;
+                    if (adapter != null && adapter.isEnabled()) {
+                        return SmartCardReaderStatus.READER_DETECTED;
+                    } else {
+                        return SmartCardReaderStatus.IDLE;
+                    }
+                })
+                .replay(1)
+                .refCount();
+    }
+
+    public Observable<SmartCardReaderStatus> status() {
+        return statusObservable;
+    }
+
+    public NfcAdapter currentAdapter() throws NFCException {
+        if (adapter == null || !adapter.isEnabled()) {
+            throw new NFCException("Reader or card is not connected");
+        }
+        return adapter;
+    }
+
+    public void disable(Activity activity) {
+        Timber.log(Log.DEBUG, "[+] disable adapter reader mode for nfc");
+        adapter.disableReaderMode(activity);
+    }
+}
