+                    Timber.log(Log.DEBUG,"[+] dataNFC on error return disable nfc");
+                    disableNFCReader();
+                    return IdCardDataResponse.failure(error, true);
+                })
+                .observeOn(AndroidSchedulers.mainThread());
+    }
