+        byte sw2 = response[response.length - 1];
+
+        //puk count does not always return 63CX but 9000 sometimes
+        if(sw1 == (byte) 0x90 && sw2 == 0x00) counts[1] = 3;
+        else counts[1] = response[response.length - 1] & 0x0f;
+
+        //Timber.log(Log.INFO, "[NFC] End PUK count");
+
+        selectFile(QSCD, "the application");
+        response = getResponse(new byte[0], emptyPIN2, "PIN2 Retry Left");
+        counts[2] = response[response.length - 1] & 0x0f;
+
+        //Timber.log(Log.INFO, "[NFC] End PIN2 count");
+
+        return counts;
+    }
